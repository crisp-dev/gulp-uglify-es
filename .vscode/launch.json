{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "request": "launch",
            "type": "node",
            "cwd": "${workspaceRoot}",
            "outDir": "${workspaceRoot}/lib",
            "program": "${workspaceRoot}/node_modules/jasmine/bin/jasmine.js",
            "sourceMaps": true,
            "stopOnEntry": false,
            "args": [],
            "preLaunchTask": "build",
            "externalConsole": false,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            }
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/lib",
            "localRoot": "${workspaceRoot}",
            "remoteRoot": null
        }
    ]
}